<div class="card">
    <div class="container">
        <form>
            <label for="userName">User Name</label>
            <input type="text" name="userName" value={{user.userName}}>

            <label for="email">Email</label>
            <input type="text" name="email" value={{user.email}}>

            <label for="city">City</label>
            <input type="text" name="city" value={{user.contact.city}}>

            <label for="state">State</label>
            <input type="text" name="state" value={{user.contact.state}}>

            <label for="zip">Zipcode</label>
            <input type="text" name="zip" value={{user.contact.zip}}>

            <label for="phoneNumber">Phone Number</label>
            <input type="tel" name="phoneNumber" id="phoneNumber" value={{user.contact.phoneNumber}}>
        </form>
    </div>
</div>

<script src="/javascript/userProfile.js"></script>